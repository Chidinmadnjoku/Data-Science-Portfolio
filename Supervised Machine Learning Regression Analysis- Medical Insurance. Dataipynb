   ],
   "source": [
    "#Fitting the nonlinear data\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "m = 100\n",
    "X = 6 * np.random.rand(m, 1) - 3\n",
    "y = 0.5 * X**2 + X + 2 + np.random.randn(m, 1)\n",
    "plt.figure()\n",
    "plt.scatter(X, y, marker = 'o', color = 'b', label='df_insurance')\n",
    "plt.legend()\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 60,
   "id": "23dd1dc4",
   "metadata": {},
   "outputs": [
